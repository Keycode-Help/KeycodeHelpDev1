# Environment Variables Template
# Copy this file to .env.local for development or .env.production for production
# Replace the placeholder values with your actual credentials

# Backend URL
VITE_BACKEND_URL=http://localhost:8080

# Environment
VITE_APP_ENV=development

# Supabase Configuration
# Get these from your Supabase project dashboard → Settings → API
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe Configuration
# Get these from your Stripe dashboard
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_test_key_here

# App Configuration
VITE_APP_NAME=Keycode Help
VITE_APP_DESCRIPTION=Professional keycode assistance for locksmiths

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true

# API Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# App Version (auto-populated from package.json)
VITE_APP_VERSION=${npm_package_version}
